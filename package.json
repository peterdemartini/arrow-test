{
  "name": "arrow-test-bundle",
  "version": "0.3.2",
  "description": "Teraslice Asset bundle for testing Apache Arrow",
  "main": "index.js",
  "scripts": {
    "asset:build": "yarn && yarn run build",
    "benchmark": "yarn build && node bench/index.js | tee -a ./BENCHMARK_RESULT.txt",
    "build": "rm -rf asset/dist; tsc --project tsconfig.json",
    "build:watch": "yarn build --watch",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "lint:fix": "yarn lint --fix",
    "test:table": "node scripts/test-table.js",
    "test": "jest",
    "test:watch": "jest --coverage=false --notify --watch --onlyChanged",
    "test:debug": "env DEBUG=\"${DEBUG:-*teraslice*}\" jest --detectOpenHandles --coverage=false --runInBand"
  },
  "private": true,
  "workspaces": [
    "asset"
  ],
  "devDependencies": {
    "@terascope/data-types": "^0.20.2",
    "@terascope/eslint-config": "^0.4.3",
    "@terascope/job-components": "^0.39.0",
    "@types/chance": "^1.1.0",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "benchmark": "^2.1.4",
    "chance": "^1.1.7",
    "eslint": "^7.6.0",
    "jest": "^26.2.2",
    "jest-extended": "^0.11.5",
    "teraslice-test-harness": "^0.25.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
